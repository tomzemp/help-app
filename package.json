{
  "name": "help-app",
  "version": "1.0.0",
  "description": "Help app allows you to define your own help topics and create custom help guides within your DHIS2 instance",
  "author": "Thomas Zemp",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "build": "d2-app-scripts build",
    "start": "d2-app-scripts start",
    "test": "d2-app-scripts test",
    "deploy": "d2-app-scripts deploy",
    "lint:js": "d2-style js check",
    "lint:text": "d2-style text check",
    "lint:staged": "yarn lint:js --staged && yarn lint:text --staged",
    "lint": "yarn lint:js",
    "format:js": "d2-style js apply",
    "format:text": "d2-style text apply",
    "format:staged": "yarn format:js --staged && yarn format:text --staged",
    "format": "yarn format:js && yarn format:text"
  },
  "devDependencies": {
    "@dhis2/cli-app-scripts": "^5.5.1",
    "@dhis2/cli-style": "^7.3.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "import": "^0.0.6"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^25.0.0",
    "@ckeditor/ckeditor5-react": "^3.0.1",
    "@dhis2/app-runtime": "^2.7.0",
    "@dhis2/ui": "^6.1.6",
    "path-browserify": "^1.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sanitize-html": "^2.3.2"
  }
}
